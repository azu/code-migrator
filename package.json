{
  "name": "code-migrator",
  "version": "1.3.2",
  "description": "Create code migration tool for your tool/library/framework.",
  "keywords": [
    "code",
    "codemod",
    "javascript",
    "migrate",
    "migration",
    "refactoring"
  ],
  "homepage": "https://github.com/azu/code-migrator",
  "bugs": {
    "url": "https://github.com/azu/code-migrator/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/code-migrator.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.2",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "inquirer-test": "^2.0.1",
    "jscodeshift": "^0.5.1",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4
  },
  "dependencies": {
    "@types/debug": "^0.0.30",
    "@types/execa": "^0.8.1",
    "@types/globby": "^6.1.0",
    "@types/inquirer": "^0.0.36",
    "@types/lodash.flatten": "^4.4.3",
    "@types/lodash.uniq": "^4.5.3",
    "@types/lodash.uniqby": "^4.7.3",
    "@types/p-all": "^1.0.0",
    "@types/read-pkg": "^3.0.0",
    "@types/semver": "^5.4.0",
    "debug": "^3.1.0",
    "execa": "^0.9.0",
    "globby": "^7.1.1",
    "inquirer": "^5.0.0",
    "is-git-clean": "^1.1.0",
    "lodash.flatten": "^4.4.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "p-all": "^1.0.0",
    "read-pkg": "^3.0.0",
    "semver": "^5.5.0"
  }
}
